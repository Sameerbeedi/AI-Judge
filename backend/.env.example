# Groq API
GROQ_API_KEY=your_groq_api_key_here

# PostgreSQL Database
DATABASE_URL=postgresql://user:password@localhost:5432/ai_judge
# For local development, use SQLite:
# DATABASE_URL=sqlite:///./ai_judge.db

# Neo4j Graph Database
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password_here
# For Neo4j Aura (cloud):
# NEO4J_URI=neo4j+s://xxxxx.databases.neo4j.io

# Redis Cache
REDIS_URL=redis://localhost:6379
# For Redis Cloud:
# REDIS_URL=redis://default:password@redis-xxxxx.cloud.redislabs.com:12345

# Qdrant Vector Database
QDRANT_URL=:memory:
# For local Docker: QDRANT_URL=http://localhost:6333
# For Qdrant Cloud: QDRANT_URL=https://xxxxx.cloud.qdrant.io
QDRANT_API_KEY=
# Only needed for Qdrant Cloud
